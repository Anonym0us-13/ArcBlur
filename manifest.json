{
    "manifest_version": 3,
    "name": "Arc with Blur",
    "version": "1.0",
    "description": "Improves look and feel of webpages. Made for Arc on Windows",
    "permissions": ["activeTab"],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["styles.css"],
            "js": ["content.js"]
        }
    ]
}
